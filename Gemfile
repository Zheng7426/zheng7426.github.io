# Bundler installs the gems and creates a Gemfile.lock
# Gemfile.lock would lock the current gem versions for a future bundle install. 
# If you ever want to update your gem versions you can run bundle update.
# frozen_string_literal: true
# Full command to start up the app: bundle exec jekyll serve

source "https://rubygems.org"

# gem "rails"
gem "jekyll"

# install the plugins using bundle update
group :jekyll_plugins do
    gem 'jekyll-sitemap'
    # gem 'jekyll-feed'
    gem 'jekyll-seo-tag'
    gem "github-pages"
end